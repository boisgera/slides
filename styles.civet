"civet autoLet"
import { create } from 'jss'

// @ts-ignore
import jss_preset from 'jss-preset-default'; preset := jss_preset.default 

jss := create(preset())

css :=    
    html:
        fontFamily: ["Inter", "sans-serif"]
        fontSize: "16px"
        fontWeight: 400
        lineHeight: 1.5
        color: "black"
        backgroundColor: "white"
    body:
        margin: 0
        padding: 0

css |> {'@global': &} |> jss.createStyleSheet |> .toString() |> console.log